{
	"name": "netbun",
	"version": "0.1.7",
	"description": "Zero-dependency socks proxy implementation for bun runtimer.",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"engines": {
		"bun": ">=1.2.0"
	},
	"scripts": {
		"build": "bun build --target=bun --minify --no-sourcemap src/index.ts --outdir=dist && bun x tsc",
		"bench": "bun run bench/proxy-bench.ts",
		"test": "bun test --timeout 25000 ./test",
		"lint": "biome check ./src --fix",
		"prepublishOnly": "bun run lint && bun run test && bun run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/phederal/netbun.git"
	},
	"bugs": {
		"url": "https://github.com/phederal/netbun/issues"
	},
	"homepage": "https://github.com/phederal/netbun#readme",
	"keywords": [
		"bun",
		"socks",
		"socks5",
		"proxy",
		"fetch",
		"http",
		"https",
		"network"
	],
	"author": "phederal",
	"license": "MIT",
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"devDependencies": {
		"@biomejs/biome": "latest",
		"@types/bun": "latest",
		"typescript": "^5.9.3"
	}
}